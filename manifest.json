{
  "name": "Profanity Filter",
  "description": "Codesmith's Hackathon",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.reddit.com/*"],
      "js": ["./main.js", "./filter.js"]
    }
  ]
}